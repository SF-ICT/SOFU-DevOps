<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Completed_Status_Needs_Finish_Date_Time</fullName>
    <active>false</active>
    <errorConditionFormula>AND
(
  ISBLANK(Finish_Date_Time__c),
  TEXT(Status__c) = &apos;Completed&apos;
)</errorConditionFormula>
    <errorDisplayField>Finish_Date_Time__c</errorDisplayField>
    <errorMessage>Please enter the Finish Date / Time to Complete this Session</errorMessage>
</ValidationRule>
