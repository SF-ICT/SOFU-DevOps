<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>BPS_Unscheduled_Review_Submitted__c</fullName>
    <formula>AND(OR(ISPICKVAL(BPS_Review_Type__c, &quot;Unscheduled&quot;), BPS_Is_Unscheduled_Review__c = TRUE), NOT(ISBLANK(BPS_Plan_Submitted_to_NDIA__c)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Unscheduled Review Submitted</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
