<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Report_Viewer_is_Owner__c</fullName>
    <description>Used in certain reports to show specific results of the owner.</description>
    <externalId>false</externalId>
    <formula>IF(Owner:User.Id = $User.Id &amp;&amp; NOT(ISBLANK(Owner:User.Id)), 1 , 0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Report Viewer is Owner</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
