<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Goal_Steps_Completed__c</fullName>
    <formula>IF(ISPICKVAL(Status__c, &quot;Completed&quot;), BPS_Current_Goal_Step__c/BPS_Total_Goal_Steps__c, (BPS_Current_Goal_Step__c - 1)/BPS_Total_Goal_Steps__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>% Goal Steps Completed</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
